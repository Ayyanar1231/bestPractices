	public static void main(String args[]) throws IOException {
		FileInputStream in = null;
	      FileOutputStream out = null;

	      try {
	         in = new FileInputStream("D:\\Methods\\Ayyanar3114NewFile");
	         out = new FileOutputStream("D:\\Methods\\AyyanarFile");
	         
	         int c;
	         while ((c = in.read()) != -1) {
	            out.write(c);
	         }
	      }
	      catch(IOException e) {
	    	  System.out.println("ERROR!!!"+e.getMessage());
	      }
	      finally {
	         if (in != null) {
	            in.close();
	         }
	         if (out != null) {
	            out.close();
	         }
	      }
	   }
}


